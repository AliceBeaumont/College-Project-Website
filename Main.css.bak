input[type=text],
select,
textarea {
  width: 50%;
  padding: 8px;
  border: 1px solid #eeeeee;
  border-radius: 4px;
  box-sizing: border-box;
  margin-top: 8px;
  margin-bottom: 16px;
  margin-right: 700px;
  resize: vertical;
}
input[type=submit] {
  background-color: #4cd9c6;
